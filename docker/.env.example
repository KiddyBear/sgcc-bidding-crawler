# 国网招标爬虫系统环境变量配置
# 复制此文件为 .env 并填写实际值

# MySQL配置
MYSQL_ROOT_PASSWORD=root123
MYSQL_DATABASE=sgcc_crawler
MYSQL_USER=inspection
MYSQL_PASSWORD=123
MYSQL_PORT=3306

# 应用端口
APP_PORT=8080

# 钉钉机器人配置
# 1. 在钉钉群中添加自定义机器人
# 2. 复制Webhook地址到下方
# 3. 如果开启了签名验证，填写Secret
DINGTALK_ENABLED=true
DINGTALK_WEBHOOK=https://oapi.dingtalk.com/robot/send?access_token=your_token_here
DINGTALK_SECRET=
